{
	"name": "fastify",
	"version": "1.0.0",
	"description": "This project was bootstrapped with Fastify-CLI.",
	"main": "app.ts",
	"scripts": {
		"build": "tsc && etsc",
		"start": "npm build && node dist/server.js",
		"build:ts": "tsc",
		"watch:ts": "tsc -w",
		"dev": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
		"dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@fastify/autoload": "^5.0.0",
		"@fastify/aws-lambda": "^3.1.3",
		"@fastify/sensible": "^5.0.0",
		"fastify": "^4.10.2",
		"fastify-cli": "^5.0.1",
		"fastify-plugin": "^4.0.0"
	},
	"devDependencies": {
		"@types/node": "^18.0.0",
		"@types/tap": "^15.0.5",
		"concurrently": "^7.0.0",
		"cpy": "^9.0.1",
		"fastify-tsconfig": "^1.0.1",
		"serverless-normal-esbuild": "^1.0.3",
		"serverless-offline": "^11.6.0",
		"tap": "^16.1.0",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.4"
	}
}
